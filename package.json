{
  "name": "react-redux-partial",
  "version": "0.0.1",
  "description": "Library to improve performance when you have many components connected to different part of the store. Also it helps to create connected components which are independent of shape of root redux store.",
  "main": "dist/index.js",
  "module": "dist-es/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.es.json",
    "version": "npm test && npm run build && conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "commit": "git-cz",
    "test": "npm run unit-test && npm run type-test",
    "unit-test": "ts-node node_modules/tape/bin/tape ./src/**/*.spec.ts",
    "type-test": "tsd",
    "tape": "ts-node node_modules/tape/bin/tape",
    "w": "nodemon -e js,jsx,ts,tsx,json --exec npm run"
  },
  "keywords": [
    "react",
    "redux",
    "subscribe",
    "performance",
    "connect"
  ],
  "author": "megazazik <megazazik@rambler.ru>",
  "license": "MIT",
  "dependencies": {
    "redux-partial": "^0.1.0"
  },
  "peerDependencies": {
    "redux": "^4.0.0",
    "react": "^16.8.0",
    "react-redux": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.44",
    "@types/react-redux": "^7.1.9",
    "@types/sinon": "^9.0.0",
    "@types/tape": "^4.2.34",
    "commitizen": "^4.0.4",
    "conventional-changelog-cli": "^2.0.34",
    "cz-conventional-changelog": "^3.0.2",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.0",
    "sinon": "^9.0.2",
    "tape": "^4.13.2",
    "ts-node": "^8.8.1",
    "tsd": "^0.13.1",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/megazazik/react-redux-partial.git"
  },
  "homepage": "https://github.com/megazazik/react-redux-partial",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "files": [
    "dist",
    "dist/index.d.ts",
    "dist-es",
    "CHANGELOG.md",
    "README.md"
  ],
  "tsd": {
    "directory": "src/tests-d",
    "compilerOptions": {
      "strict": false,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "strictNullChecks": true
    }
  },
  "sideEffects": false
}